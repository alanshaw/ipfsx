{
  "name": "ipfsx",
  "version": "0.12.0",
  "description": "Experimental IPFS API",
  "main": "src/index.js",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && run-p --race test:*",
    "test:portservice": "node test/helpers/port-service/bin",
    "test:runner": "run-s test:runner:embedded",
    "test:runner:embedded": "ava --verbose",
    "test:runner:daemon": "IPFS_TYPE=daemon ava --verbose",
    "coverage": "nyc --reporter=lcov --reporter=text ava --verbose"
  },
  "author": "Alan Shaw",
  "license": "MIT",
  "dependencies": {
    "async-iterator-to-pull-stream": "^1.1.0",
    "cids": "^0.5.4",
    "debug": "^4.0.1",
    "ipfs-block": "^0.7.1",
    "pull-stream-to-async-iterator": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.8",
    "babel-eslint": "^10.0.0",
    "deepmerge": "^2.1.1",
    "go-ipfs-dep": "^0.4.17",
    "got": "^9.2.1",
    "ipfs": "^0.32.2",
    "ipfs-api": "^24.0.2",
    "js-unixfsv2-draft": "^0.2.0",
    "multiaddr": "^5.0.0",
    "multiaddr-to-uri": "^4.0.0",
    "multihashes": "^0.4.14",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.0.1",
    "port-manager": "^1.0.1",
    "promise-queue": "^2.2.5",
    "pull-stream": "^3.6.9",
    "rmfr": "^2.0.0",
    "shortid": "^2.2.13",
    "standard": "^12.0.1",
    "start-ipfs-daemon": "^1.0.1",
    "underscore": "^1.9.1"
  },
  "peerDependencies": {
    "ipfs": "latest"
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "failWithoutAssertions": false
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
